{
  "name": "ai-log-analyzer",
  "version": "0.0.3",
  "description": "Web-only app that analyzes logs using AI to identify errors and notable patterns",
  "author": "Sudhanshu Gautam",
  "license": "MIT",
  "keywords": ["log", "analyzer", "log-analyzer", "ai", "web"],
  "repository": {
    "type": "git",
    "url": "https://github.com/sudhanshug16/log-analyzer.git"
  },
  "scripts": {
    "dev": "bun ./src/index.html --console",
    "build:css": "bunx @tailwindcss/cli -i ./src/styles.css -o ./dist/styles.css --minify",
    "build": "rm -rf dist && bun build ./src/index.html --outdir dist --minify",
    "preview": "bun run serve.ts",
    "start": "bun run serve.ts"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.10",
    "@types/bun": "latest",
    "bun-plugin-tailwind": "^0.0.15",
    "daisyui": "^5.0.43",
    "tailwindcss": "^4.1.0"
  },
  "type": "module",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "ai": "^4.3.16",
    "gpt-tokenizer": "^2.9.0"
  }
}
